  "bugs": {
    "url": "https://github.com/akwong2/bulkmailcenter/issues"
  },
  "homepage": "https://github.com/akwong2/bulkmailcenter#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "path": "^0.12.7"
  }
